{
  "name": "recjson",
  "version": "1.0.2",
  "description": "recursive javascript objects made non-recursive",
  "main": "dist/lib.js",
  "module": "dist/lib.esm.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build:docs": "typedoc --out docs --tsconfig doctsconfig.json dist/main.d.ts",
    "test": "jest",
    "prepublishOnly": "npm run build && npm run build:docs"
  },
  "exports": {
    ".": {
      "import": "./dist/lib.esm.js",
      "require": "./dist/lib.js",
      "types": "./dist/main.d.ts"
    }
  },
  "keywords": [
    "recursive",
    "circular",
    "json",
    "serialization",
    "deserialization",
    "object",
    "javascript",
    "js",
    "recjson"
  ],
  "author": "Expo",
  "maintainers": [
    {
      "name": "3xpo",
      "email": "expo@expo.moe"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@3xpo/microjson": "^1.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "rollup": "^3.27.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Exponential-Workload/recjson.git"
  },
  "homepage": "https://gh.expo.moe/recjson"
}